<AnchorPane fx:id="WelcomePage" prefHeight="600.0" prefWidth="1060.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.tootGarment.controller.WelcomePageFormController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1060.0">
         <image>
            <Image url="@../icons/wall.jpg" />
         </image>
      </ImageView>
      <Button layoutX="116.0" layoutY="523.0" mnemonicParsing="false" onAction="#LoginOnAction" prefHeight="25.0" prefWidth="114.0" style="-fx-background-color: #0099FF; -fx-background-radius: 20;" text="LOGIN" textFill="WHITE">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
